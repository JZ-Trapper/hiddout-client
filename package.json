{
  "name": "hiddout-client",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "main": "index.html",
  "window": {
    "title": "Hiddout",
    "icon": "assets/Hiddout.png",
    "min_width": 1072,
    "min_height": 764
  },
  "chromium-args": "--disable-raf-throttling",
  "scripts": {
    "build": "node ./devScript/dist.js",
    "build-web": "webpack --config config/webpack.web.prod.js && node ./devScript/webDist.js",
    "build-dev": "webpack --config config/webpack.client.js",
    "build-prod": "webpack --config config/webpack.prod.js",
    "flow": "flow",
    "lint": "eslint --ext .js src",
    "lint-fix": "eslint --ext .js src --fix",
    "postinstall": "node ./devScript/init.js",
    "rebuild": "node ./devScript/rebuild.js",
    "run-mac": "./nwjs-sdk-v0.30.1-osx-x64/nwjs.app/Contents/MacOS/nwjs .",
    "run-win": "nwjs-sdk-v0.30.1-win-x64\\nw.exe .",
    "dev": "node ./devScript/dev.js",
    "dev-mac": "npm run build-dev && npm run run-mac",
    "dev-win": "npm run build-dev && npm run run-win",
    "test": "npm run unit-testing && npm run flow check && npm run lint && npm run build-prod",
    "unit-testing": "mocha test/**/*.test.js --require @babel/register",
    "web": "webpack-dev-server --open --config config/webpack.web.js"
  },
  "prettier": {
    "useTabs": true,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always",
    "parser": "flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiddout/hiddout-client.git"
  },
  "author": "Tyreal Gray",
  "license": "AFL-3.0",
  "bugs": {
    "url": "https://github.com/hiddout/hiddout-client/issues"
  },
  "homepage": "https://github.com/hiddout/hiddout-client#readme",
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.2",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "circular-dependency-plugin": "5.0.2",
    "cli-spinner": "0.2.8",
    "commander": "2.19.0",
    "core-js": "2.6.4",
    "css-loader": "2.1.0",
    "download": "7.1.0",
    "eslint": "5.13.0",
    "eslint-plugin-react": "7.12.4",
    "expect.js": "0.3.1",
    "file-loader": "3.0.1",
    "flow-bin": "0.92.1",
    "fs-extra": "7.0.1",
    "mocha": "5.2.0",
    "nw-gyp": "3.6.3",
    "plist": "3.0.1",
    "prettier": "1.16.4",
    "rcedit": "1.1.1",
    "redux-devtools-extension": "^2.13.8",
    "style-loader": "0.23.1",
    "webpack": "4.29.3",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.2.1"
  },
  "dependencies": {
    "connected-react-router": "6.3.1",
    "hiddout-viewer": "0.1.4",
    "i18next": "15.0.6",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-redux": "6.0.1",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-persist": "^5.10.0",
    "semantic-ui-react": "0.85.0"
  }
}
